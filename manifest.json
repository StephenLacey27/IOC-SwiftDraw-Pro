{
  "manifest_version": 3,
  "name": "IOC-SwiftDraw Pro",
  "version": "1.0.0",
  "description": "Fast, secure OSINT context menu extension for IOCs with customizable SwiftDraw Pro.",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    },
    "default_title": "IOC-SwiftDraw Pro"
  }
}
